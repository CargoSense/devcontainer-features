{
    "name": "ShellCheck",
    "id": "shellcheck",
    "version": "1.1.0",
    "description": "Install ShellCheck, a static analysis tool for shell scripts.",
    "documentationURL": "https://github.com/CargoSense/devcontainer-features/tree/main/src/shellcheck",
    "licenseURL": "https://github.com/CargoSense/devcontainer-features/blob/main/LICENSE",
    "options": {
      "version": {
        "type": "string",
        "proposals": [
          "latest",
          "os-provided"
        ],
        "default": "os-provided",
        "description": "Select or enter a ShellCheck version."
      },
      "installPath": {
        "type": "string",
        "default": "/usr/local/bin",
        "description": "The path where ShellCheck will be installed."
      }
    },
    "installsAfter": [
      "ghcr.io/devcontainers/features/common-utils"
    ]
  }
